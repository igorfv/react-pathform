{
  "name": "pathform",
  "version": "0.0.1",
  "author": "Ian Lovett",
  "repository": "github:ilovett/react-pathform",
  "homepage": "https://github.com/ilovett/react-pathform",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "module": "dist/react-pathform.esm.js",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "dependencies": {
    "lodash": "^4.17.20",
    "lodash-es": "^4.17.20",
    "uuid": "^3.4.0"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "devDependencies": {
    "@size-limit/preset-small-lib": "^4.9.2",
    "@types/lodash": "^4.14.168",
    "@types/react": "^16.0.0",
    "@types/react-dom": "^16.0.0",
    "@types/uuid": "^8.3.0",
    "husky": "^5.0.9",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "size-limit": "^4.9.2",
    "tsdx": "^0.14.1",
    "tslib": "^2.1.0",
    "typescript": "^4.1.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 140,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5",
    "arrowParens": "always"
  },
  "size-limit": [
    {
      "path": "dist/react-pathform.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/react-pathform.esm.js",
      "limit": "10 KB"
    }
  ]
}
